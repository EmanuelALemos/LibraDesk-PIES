                     textFill="WHITE">
                     <font>
                        <Font name="Segoe UI Black" size="16.0" />
                     </font>
                  </Button>
                  <Button id="botaoEntrar" maxHeight="34.0" maxWidth="110.0" mnemonicParsing="false"
                     onAction="#btEntrar" prefHeight="34.0" prefWidth="102.0" text="confirmar"
                     textFill="WHITE">
                     <font>
                        <Font name="Segoe UI Black" size="16.0" />
                     </font>
                  </Button>
               </children>
               <padding>
                  <Insets top="8.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
