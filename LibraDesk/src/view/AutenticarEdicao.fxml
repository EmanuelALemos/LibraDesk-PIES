         AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Confirme que é você">
               <font>
                  <Font name="Segoe UI Semibold" size="32.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="8.0" />
               </VBox.margin>
            </Text>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="senha">
               <font>
                  <Font name="Segoe UI Semilight" size="16.0" />
               </font>
            </Text>
            <TextField id="inserirDado" maxHeight="28.0" maxWidth="320.0" minHeight="28.0"
